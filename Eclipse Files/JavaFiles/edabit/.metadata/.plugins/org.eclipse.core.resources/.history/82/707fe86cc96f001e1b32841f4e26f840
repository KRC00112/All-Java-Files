package num1_howMuchIsTrue;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n=0;
		boolean[] array= {true,false,false,true,false};
		
		for(int i=0;i<array.length;i++) {
			
			if(array[i]==true) {
				n++;
			}
			
			
		}
		
		System.out.println(n);
	}

}
