package num7_FizzBuzz;

public class Main {

	public static void main(String[] args) {

		Integer x=15;
		
		
		
		
		 if(x%3==0) {
			System.out.print("Fizz");
		}
		 else if(x%5==0) {
			System.out.println("Buzz");
		}
		  if(x%5==0 && x%3==0) {
				System.out.println("FizzBuzz");
			}
		 else if(x%5!=0 && x%3!=0) {
				System.out.println(x.toString());
			}
		
		 
		
		
		
		
	}

}
