package num7_FizzBuzz;

public class Main {

	public static void main(String[] args) {

		Integer x=3;
		int num=1;
		int i=0;
		
		while(i<num) {
		 if(x%3==0) {
			System.out.print("Fizz");
		}
		 if(x%5==0) {
			System.out.println("Buzz");
		}
		 i++;
		}
		
		 if(x%3!=0||x%5!=0) {
				System.out.println(x.toString()); 
		}
		
		
		
		
	}

}
