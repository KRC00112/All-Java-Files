package num2_areTheNumbersEqual;

public class Main {

	public static void main(String[] args) {

	
		
		isEqual(8,10);
		
		
		
		
	}
	
	static boolean isEqual(int num1,int num2) {
		
		boolean choice=true;
		
		if(num1==num2) {
			choice=true;
			System.out.println(choice);
		}
		
		else if(num1!=num2) {
			choice=false;
			System.out.println(choice);
			
		}
		
		return choice;
		
		
	
	}
	

}
