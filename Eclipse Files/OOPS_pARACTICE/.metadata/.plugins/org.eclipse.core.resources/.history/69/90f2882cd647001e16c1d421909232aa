package classes;

public class Person {
	
	


	void get(String name,int age,double height,double weight) {
		System.out.println("His name is "+name);
		System.out.println("he is "+age+" years old");
		System.out.println(name+" is "+height+"cm tall");
		System.out.println(name+" weighs"+weight+"kgs");
	
	
	
	}
	
	
	
	
	
	
	
	

}
