package fizzBuzz;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
	
			
			for(int i=1;i<=15;i++) {
				
				
				if(i%3==0) {
				
					System.out.println("Fizz");
					
					if(i%5==0) {
						System.out.println("Buzz");
					}
					
				}
				
				
				
				
				
				
				
			}
			
			
			
			
		
		
		

	}

}
