package fizzBuzz;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
			
			for(int i=1;i<=15;i++) {
			
				String fizzBuzz=String.valueOf(i);
				
				
				if(i%3==0) {
					
					fizzBuzz="Fizz";
				}
				if(i%5==0) {
					
					fizzBuzz="Buzz";
				}
				if(i%5==0 && i%3==0) {
					fizzBuzz="FizzBuzz";
				}
				
				
				System.out.println(fizzBuzz);
				
				
				
				
				
			}
			
			
			
			
		
		
		

	}

}
