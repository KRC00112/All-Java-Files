package fizzBuzz;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		String fb="";
			
			for(int i=1;i<=15;i++) {
				
				
				if(i%3==0) {
				
					fb="Fizz";
					
				}
				if(i%5==0) {
					fb="Buzz";
				}
				
				
				System.out.println(fb);
				
				
				
			}
			
			
			
			
		
		
		

	}

}
