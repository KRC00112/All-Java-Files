package palindrome;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String x="kaustav";
		
		String hold;
		
		for(int i=x.length()-1;i>=0;i--) {
			
			
			hold=""+x.charAt(i);
			
			System.out.print(hold);
			
			
			
			
			
			
		}
		
		
		
		

	}

}
