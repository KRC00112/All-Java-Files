
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		int[] array= {3,2,3,1,2};
		
		int count=0;
	
		
		for(int i=0;i<array.length;i++) {
			
		for(int j=0;j<i;j++) {
			
			if(array[j]==i) {
			
				System.out.println(j);
				
			}
			
			
		}
			
		}
		
		

	}

}
