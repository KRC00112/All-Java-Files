package reverseANumber;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int x=29;
		
		String l=String.valueOf(x);
		
		
		String[] numbers=l.split("");
		
		
		for(int i=numbers.length-1;i>=0;i--) {
			
			
			System.out.println(numbers[i]);
			
		}
		

		
		
		
		
		
		

	}

}
