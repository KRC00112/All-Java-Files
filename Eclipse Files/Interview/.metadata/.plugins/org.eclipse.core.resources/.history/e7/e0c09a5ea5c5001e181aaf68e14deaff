package palindrome;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String x="kaustav";
		
		String hold = null;
		
		for(int i=x.length()-1;i>=0;i--) {
			
			
			hold=hold+x.charAt(i);
			
			
			
			
			
			
			
		}
		
		System.out.print(hold);

		
		
		
		

	}

}
